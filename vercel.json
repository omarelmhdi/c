{
  "version": 2,

  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "WEBHOOK_URL": "@webhook_url"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  }
}
